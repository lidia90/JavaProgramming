package practice1;

public class Greeting_main 
{

	public static void main(String[] args) 
	{
		int a[]= {1, 23, 12, 9, 30, 2, 50},t=0,k=4;
		
		//System.out.println("leargest no is:");
		
		for(int i=0;i<k;i++)
			for(int j=i+1;j<k;j++)
			{
				if(a[i]>a[j])
				{
					t=a[i];
					a[i]=a[j];
					a[j]=t;
				
				}
			}
			System.out.println("largest no is:" + a[k-1]) ;
		
	}
		
}

