package practice;

import java.util.Arrays;

public class SortingElementArray {

	public static void main(String[] args) 
	{
		int a[]= {2,5,1,3,12,10,2};
		
		for (int i=0;i<a.length;i++)
		
			for(int j=i+1;j<a.length;j++)
			
				if(a[j]<a[i])
				{
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			
		
		System.out.println(a[a.length-1]);
	}

}
