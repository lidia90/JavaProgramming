package Udemy;

import java.util.Scanner;

public class PrintNoinWord {

	public static void main(String[] args) 
	{
		 Scanner sc = new Scanner(System.in);
		 System.out.println("enter a no:");
		 int n = sc.nextInt();
		 int rem;
		 String str="";
		 while(n>0)
		 {
			 rem=n%10;
			 n=n/10;
			 str=str+rem;
		 }
		 System.out.println(str);

	}

}
